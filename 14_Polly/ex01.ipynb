   "source": [
    "import boto3\n",
    "\n",
    "polly = boto3.client('polly')\n",
    "response = polly.synthesize_speech(\n",
    "    Text='Hello, world!',\n",
    "    OutputFormat='mp3',\n",
    "    VoiceId='Joanna'\n",
    ")\n",
    "with open('output.mp3', 'wb') as file:\n",
    "    file.write(response['AudioStream'].read())"
   ]
  }
